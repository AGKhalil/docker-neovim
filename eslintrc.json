{
  "extends": "airbnb",
  "rules": {
    "no-undef": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0,
    "require-jsdoc": ["error", {
      "require": {
        "FunctionDeclaration": true,
        "MethodDefinition": false,
        "ClassDeclaration": false
      }
    }],
    "valid-jsdoc": ["error", {
      "requireParamDescription": true,
      "requireReturn": false
    }]
  }
}
